<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite default-match-type="wildcard"> 
 <rule>
  <from>/notice</from>
  <to>/board/notice?nav=10000&amp;cla=1000&amp;mid=100</to>
 </rule>
 <rule>
  <from>/notice/register</from>
  <to>/board/register?nav=10000</to>
 </rule>
<rule>
  <from>/notice/**/insert</from>
  <to>/board/insert?nav=10000</to>
 </rule>
  <rule>
  <from>/notice/modify</from>
  <to>/board/modify?nav=10000</to>
 </rule>
  <rule>
  <from>/notice/commentaire</from>
  <to>/board/commentaire?nav=10000</to>
 </rule>

 <rule>
  <from>/cmu/total</from>
  <to>/board/brdnormal?nav=20000&amp;cla=1100</to>
 </rule>
 <rule>
  <from>/cmu/hot</from>
  <to>/board/brdnormal?nav=20000&amp;cla=1200</to>
 </rule>
 <rule>
  <from>/cmu/brd**</from>
  <to>/board/brdcate?nav=20000</to>
 </rule>
 <rule>
  <from>**/cmu/register</from>
  <to>/board/register?nav=20000</to>
 </rule>
<rule>
  <from>**/cmu/**/insert</from>
  <to>/board/insert?nav=20000</to>
 </rule>
  <rule>
  <from>**/cmu/modify</from>
  <to>/board/modify?nav=20000</to>
 </rule>
  <rule>
  <from>**/cmu/commentaire</from>
  <to>/board/commentaire?nav=20000</to>
 </rule>
